<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.xmzlb.wine.RegisterActivity" >

    <RelativeLayout
        android:id="@+id/bar"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/wine" >

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="新用户注册"
            android:textColor="@color/white"
            android:textSize="20sp" />

        <RelativeLayout
            android:id="@+id/back"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:visibility="gone" >

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:padding="@dimen/ima_back_bar_padding"
                android:src="@drawable/selector_ima_back_bar" />
        </RelativeLayout>
    </RelativeLayout>

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/bar"
        android:layout_centerHorizontal="true"
        android:background="@color/white"
        android:scrollbars="none" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="20dp"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_centerHorizontal="true"
                    android:padding="10dp"
                    android:src="@drawable/logo_red"
                    android:visibility="visible" />
            </RelativeLayout>

            <RelativeLayout
                    android:id="@+id/rela_realname"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/marginleft_register"
                    android:layout_marginRight="@dimen/marginright_register" >

                    <TextView
                        android:id="@+id/textview0"
                        android:layout_width="@dimen/text_width_register"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="用户姓名："
                        android:textColor="@color/black"
                        android:textSize="@dimen/textsize_registe" />

                    <EditText
                        android:id="@+id/edit_realname"
                        android:layout_width="match_parent"
                        android:paddingLeft="5dp"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/textview0"
                        android:background="@drawable/shape_topcorner_strokegray_white"
                        android:ems="10"
                        android:textSize="@dimen/textsize_registe" >
                    </EditText>
                </RelativeLayout>
            
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <RelativeLayout
                    android:id="@+id/rela_phone1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/marginleft_register"
                    android:layout_marginRight="@dimen/marginright_register" >

                    <TextView
                        android:id="@+id/textview1"
                        android:layout_width="@dimen/text_width_register"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="手机号码："
                        android:textColor="@color/black"
                        android:textSize="@dimen/textsize_registe" />

                    <EditText
                        android:id="@+id/edit_phone"
                        android:layout_width="match_parent"
                        android:paddingLeft="5dp"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/textview1"
                        android:background="@drawable/shape_topcorner_strokegray_white"
                        android:ems="10"
                        android:inputType="number"
                        android:textSize="@dimen/textsize_registe" >
                    </EditText>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/rela_phone1"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/marginleft_register"
                    android:layout_marginRight="@dimen/marginright_register"
                    android:layout_marginTop="0dp" >

                    <TextView
                        android:id="@+id/textview2"
                        android:layout_width="@dimen/text_width_register"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="手机号码："
                        android:textColor="@color/black"
                        android:textSize="@dimen/textsize_registe"
                        android:visibility="invisible" />

                    <TextView
                        android:id="@+id/text_getSMS"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/textview2"
                        android:gravity="left|center_vertical"
                        android:text="获取短信验证"
                        android:textSize="@dimen/textsize_registe" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/marginleft_register"
                    android:layout_marginRight="@dimen/marginright_register" >

                    <TextView
                        android:id="@+id/textview3"
                        android:layout_width="@dimen/text_width_register"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="短信验证码："
                        android:textColor="@color/black"
                        android:textSize="@dimen/textsize_registe" />

                    <EditText
                        android:id="@+id/edit_smsnum_register"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:paddingLeft="5dp"
                        android:layout_marginLeft="10dp"
                        android:inputType="number"
                        android:layout_toRightOf="@+id/textview3"
                        android:background="@drawable/shape_topcorner_strokegray_white"
                        android:ems="10"
                        android:textSize="@dimen/textsize_registe" >
                    </EditText>
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/marginleft_register"
                    android:layout_marginRight="@dimen/marginright_register" >

                    <TextView
                        android:id="@+id/textview4"
                        android:layout_width="@dimen/text_width_register"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="用户名："
                        android:textColor="@color/black"
                        android:textSize="@dimen/textsize_registe" />

                    <EditText
                        android:id="@+id/edit_username_register"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:paddingLeft="5dp"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/textview4"
                        android:background="@drawable/shape_topcorner_strokegray_white"
                        android:ems="10"
                        android:textSize="@dimen/textsize_registe" >
                    </EditText>
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/marginleft_register"
                    android:layout_marginRight="@dimen/marginright_register" >

                    <TextView
                        android:id="@+id/textview5"
                        android:layout_width="@dimen/text_width_register"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="初设密码："
                        android:textColor="@color/black"
                        android:textSize="@dimen/textsize_registe" />

                    <EditText
                        android:id="@+id/edit_pwd1_register"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:paddingLeft="5dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/textview5"
                        android:background="@drawable/shape_topcorner_strokegray_white"
                        android:ems="10"
                        android:inputType="textPassword"
                        android:textSize="@dimen/textsize_registe" >
                    </EditText>
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/marginleft_register"
                    android:layout_marginRight="@dimen/marginright_register" >

                    <TextView
                        android:id="@+id/textview6"
                        android:layout_width="@dimen/text_width_register"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="确认密码："
                        android:textColor="@color/black"
                        android:textSize="@dimen/textsize_registe" />

                    <EditText
                        android:id="@+id/edit_pwd2_register"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:paddingLeft="5dp"
                        android:layout_toRightOf="@+id/textview6"
                        android:background="@drawable/shape_topcorner_strokegray_white"
                        android:inputType="textPassword"
                        android:ems="10"
                        android:textSize="@dimen/textsize_registe" >
                    </EditText>
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/marginleft_register"
                    android:layout_marginRight="@dimen/marginright_register" >

                    <TextView
                        android:id="@+id/textview7"
                        android:layout_width="@dimen/text_width_register"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="会员生日："
                        android:textColor="@color/black"
                        android:textSize="@dimen/textsize_registe" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/textview7"
                        android:orientation="horizontal" >

                        <EditText
                            android:id="@+id/edit_birthyear_register"
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/shape_topcorner_strokegray_white"
                            android:gravity="center"
                            android:hint=" "
                            android:inputType="number"
                            android:textSize="@dimen/textsize_registe" >
                        </EditText>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="年"
                            android:textColor="@color/black"
                            android:textSize="@dimen/textsize_registe" />

                        <EditText
                            android:id="@+id/edit_birthmonth_register"
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/shape_topcorner_strokegray_white"
                            android:gravity="center"
                            android:hint=" "
                            android:inputType="number"
                            android:textSize="@dimen/textsize_registe" >
                        </EditText>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="月"
                            android:textColor="@color/black"
                            android:textSize="@dimen/textsize_registe" />

                        <EditText
                            android:id="@+id/edit_birthday_register"
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/shape_topcorner_strokegray_white"
                            android:gravity="center"
                            android:hint=" "
                            android:inputType="number"
                            android:textSize="@dimen/textsize_registe" >
                        </EditText>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="日"
                            android:textColor="@color/black"
                            android:textSize="@dimen/textsize_registe" />
                    </LinearLayout>
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/marginleft_register"
                    android:layout_marginRight="@dimen/marginright_register" >

                    <TextView
                        android:id="@+id/textview8"
                        android:layout_width="@dimen/text_width_register"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="店铺名称："
                        android:textColor="@color/black"
                        android:textSize="@dimen/textsize_registe" />

                    <EditText
                        android:id="@+id/edit_storename_register"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:paddingLeft="5dp"
                        android:layout_toRightOf="@+id/textview8"
                        android:background="@drawable/shape_topcorner_strokegray_white"
                        android:ems="10"
                        android:textSize="@dimen/textsize_registe" >
                    </EditText>
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/marginleft_register"
                    android:layout_marginRight="@dimen/marginright_register" >

                    <TextView
                        android:id="@+id/textview9"
                        android:layout_width="@dimen/text_width_register"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="店铺地址："
                        android:textColor="@color/black"
                        android:textSize="@dimen/textsize_registe" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/textview9"
                        android:orientation="horizontal" >

                        <RelativeLayout
                            android:id="@+id/rela_shen"
                            android:layout_width="fill_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/shape_topcorner_strokegray_white" >

                            <TextView
                                android:id="@+id/textview_province"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:padding="2dp"
                                android:textColor="@color/black"
                                android:text="请选择" />

                            <ImageView
                                android:id="@+id/imageView3"
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:src="@drawable/sorticon" />
                        </RelativeLayout>

                        <TextView
                            android:layout_width="10dp"
                            android:layout_height="1dp"
                            android:background="@color/white" />

                        <RelativeLayout
                            android:id="@+id/rela_shi"
                            android:layout_width="fill_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/shape_topcorner_strokegray_white" >

                            <TextView
                                android:id="@+id/textview_city"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:textColor="@color/black"
                                android:padding="2dp"
                                android:text="请选择" />

                            <ImageView
                                android:id="@+id/imageView4"
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:src="@drawable/sorticon" />
                        </RelativeLayout>

                        <TextView
                            android:layout_width="10dp"
                            android:layout_height="1dp"
                            android:background="@color/white" />

                        <RelativeLayout
                            android:id="@+id/rela_xian"
                            android:layout_width="fill_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/shape_topcorner_strokegray_white" >

                            <TextView
                                android:id="@+id/textview_county"
                                android:layout_width="wrap_content"
                                android:textColor="@color/black"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:padding="2dp"
                                android:text="请选择" />

                            <ImageView
                                android:id="@+id/imageView5"
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:src="@drawable/sorticon" />
                        </RelativeLayout>
                    </LinearLayout>
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/marginleft_register"
                    android:layout_marginRight="@dimen/marginright_register" >

                    <TextView
                        android:id="@+id/textview10"
                        android:layout_width="@dimen/text_width_register"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="店铺名称："
                        android:textColor="@color/black"
                        android:textSize="@dimen/textsize_registe"
                        android:visibility="invisible" />

                    <EditText
                        android:id="@+id/edit_storeaddress_register"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/textview10"
                        android:background="@drawable/shape_topcorner_strokegray_white"
                        android:ems="10"
                        android:hint="详细地址"
                        android:paddingLeft="5dp"
                        android:textSize="@dimen/textsize_registe" >
                    </EditText>
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/marginleft_register"
                    android:layout_marginRight="@dimen/marginright_register" >

                    <TextView
                        android:id="@+id/textview11"
                        android:layout_width="@dimen/text_width_register"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="营业执照："
                        android:textColor="@color/black"
                        android:textSize="@dimen/textsize_registe" />

                    <EditText
                        android:id="@+id/edit_sellNum_register"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/textview11"
                        android:background="@drawable/shape_topcorner_strokegray_white"
                        android:ems="10"
                        android:hint="如有请填入"
                        android:paddingLeft="5dp"
                        android:textSize="@dimen/textsize_registe" >
                    </EditText>
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/marginleft_register"
                    android:layout_marginRight="@dimen/marginright_register" >

                    <TextView
                        android:id="@+id/textview12"
                        android:layout_width="@dimen/text_width_register"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="注册业务员："
                        android:textColor="@color/black"
                        android:textSize="@dimen/textsize_registe" />

                    <EditText
                        android:id="@+id/edit_sellPeople_register"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:paddingLeft="5dp"
                        android:layout_toRightOf="@+id/textview12"
                        android:background="@drawable/shape_topcorner_strokegray_white"
                        android:ems="10"
                        android:textSize="@dimen/textsize_registe" >
                    </EditText>
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/marginleft_register"
                    android:layout_marginRight="@dimen/marginright_register" >

                    <TextView
                        android:id="@+id/textview13"
                        android:layout_width="@dimen/text_width_register"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="邀请SN码："
                        android:textColor="@color/black"
                        android:textSize="@dimen/textsize_registe" />

                    <EditText
                        android:id="@+id/edit_snNum_register"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:paddingLeft="5dp"
                        android:layout_toRightOf="@+id/textview13"
                        android:background="@drawable/shape_topcorner_strokegray_white"
                        android:ems="10"
                        android:textSize="@dimen/textsize_registe" >
                    </EditText>
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:orientation="horizontal" >

                    <CheckBox
                        android:id="@+id/checkbox"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true" />

                    <TextView
                        android:id="@+id/textview14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/checkbox"
